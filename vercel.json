{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "prisma generate && next build"
      }
    }
  ],
  "env": {
    "NEXTAUTH_URL": {
      "type": "string"
    },
    "NEXTAUTH_SECRET": {
      "type": "string"
    },
    "GITHUB_ID": {
      "type": "string"
    },
    "GITHUB_SECRET": {
      "type": "string"
    }
  }
}